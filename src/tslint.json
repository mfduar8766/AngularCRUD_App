{
    "extends": "../tslint.json",
    "rules": {
      "whitespace":false,
      "no-trailing-whitespace":false,
      "comment-format":false,
      "max-line-length":false,
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ]
    }
}
